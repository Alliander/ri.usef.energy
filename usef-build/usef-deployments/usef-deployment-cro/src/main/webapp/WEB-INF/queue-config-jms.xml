<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<messaging-deployment xmlns="urn:jboss:messaging-deployment:1.0">
	<hornetq-server>
		<jms-destinations>
			<jms-queue name="IN_QUEUE_CRO">
				<entry name="java:jboss/exported/jms/usefInQueueCro" />
				<durable>true</durable>
			</jms-queue>
			<jms-queue name="OUT_QUEUE_CRO">
				<entry name="java:jboss/exported/jms/usefOutQueueCro" />
				<durable>true</durable>
			</jms-queue>
			<jms-queue name="NOT_SENT_QUEUE_CRO">
				<entry name="java:jboss/exported/jms/usefNotSentQueueCro" />
				<durable>true</durable>
			</jms-queue>
		</jms-destinations>
	</hornetq-server>
</messaging-deployment>