<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<messaging-deployment xmlns="urn:jboss:messaging-deployment:1.0">
	<hornetq-server>
		<jms-destinations>
			<jms-queue name="IN_QUEUE_BRP">
				<entry name="java:jboss/exported/jms/usefInQueueBrp" />
				<durable>true</durable>
			</jms-queue>
			<jms-queue name="OUT_QUEUE_BRP">
				<entry name="java:jboss/exported/jms/usefOutQueueBrp" />
				<durable>true</durable>
			</jms-queue>
			<jms-queue name="NOT_SENT_QUEUE_BRP">
				<entry name="java:jboss/exported/jms/usefNotSentQueueBrp" />
				<durable>true</durable>
			</jms-queue>
		</jms-destinations>
	</hornetq-server>
</messaging-deployment>